\name{rel-package}
\alias{rel}
\alias{rel-package}
\docType{package}
\title{Various reliability coefficients}
\description{
  \code{rel} provides functions for evaluating agreement between measurements: Bennett et als S (\code{\link{bS}}), Cohen's kappa and weighted kappa (\code{\link{cKap}}), Gwet's AC1 and AC2 (\code{\link{gAC}}), Krippendorff's alpha (\code{\link{krA}}), and Scott's pi (\code{\link{sPi}}).
}
\keyword{ 
  package
  univar
}